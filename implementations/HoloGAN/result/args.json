{
  "name": "HoloGAN",
  "disable_gpu": false,
  "disable_amp": false,
  "dataset": "animeface",
  "image_size": 128,
  "batch_size": 32,
  "min_year": 2005,
  "num_images": 60000,
  "save": 1000,
  "max_iters": -1,
  "default_epochs": 100,
  "g_channels": 512,
  "d_channels": 64,
  "latent_dim": 128,
  "activation": "lrelu",
  "epochs": 150,
  "lr": 0.0001,
  "beta1": 0.5,
  "beta2": 0.999,
  "policy": "color,translation",
  "style_lambda": 1.0,
  "identity_lambda": 1.0,
  "eval_size": 10
}